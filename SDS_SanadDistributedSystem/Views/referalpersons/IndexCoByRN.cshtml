                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.outreachnote)
                                    </th>

                                    @*<th>تعديل</th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @try
                                {
                                    foreach (SDS_SanadDistributedSystem.Models.temporal item in ViewBag.temporal)
                                    {
                                        if (item.servicestate == "Closed" && item.referalstate == "Approved")
                                        {
                                            <tr>
                                                <td>
                                                    @item.fname @item.lname
                                                </td>
                                                @if (item.submittingdate == null)
                                                {
                                                    <td>
                                                        @item.submittingdate
                                                    </td>    }
                                                else
                                                {
                                                    <td>
                                                        @item.submittingdate.Value.ToShortDateString()
                                                    </td>
                                                }

                                                @if (item.referaldate == null)
                                                {
                                                    <td>
                                                        @item.referaldate
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td>
                                                        @item.referaldate.Value.ToShortDateString()
                                                    </td>
                                                }

                                                @if (item.servicestartdate == null)
                                                {
                                                    <td>
                                                        @item.servicestartdate
                                                    </td>    }
                                                else
                                                {
                                                    <td>
                                                        @item.servicestartdate.Value.ToShortDateString()
                                                    </td>
                                                }

                                                @if (item.serviceenddate == null)
                                                {
                                                    <td>
                                                        @item.serviceenddate
                                                    </td>    }
                                                else
                                                {
                                                    <td>
                                                        @item.serviceenddate.Value.ToShortDateString()
                                                    </td>
                                                }
                                                <td>
                                                    @item.AspNetUser1.Email
                                                </td>
                                                <td>
                                                    @item.senderevalution
                                                </td>
                                                <td>
                                                    @item.recieverevalution
                                                </td>
                                                <td>
                                                    @item.outreachnote
                                                </td>
                                                @*<td>
                                                        @Html.ActionLink("تعديل", "EditReferal", "temporals", new { id = @item.idperson }, "")

                                                    </td>*@
                                            </tr>
                                        }
                                    }
                                }
                                catch (Exception e)
                                {
                                    Console.Write("" + e);
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        }
    </div>




